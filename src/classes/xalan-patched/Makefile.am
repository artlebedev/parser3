INCLUDES = -I../../types @XML_INCLUDES@

noinst_LIBRARIES = libxalan_patched.a
libxalan_patched_a_SOURCES = XalanTransformer2.cpp ident.C

IDENT_AWK = ../../ident.awk
IDENT_DEPS = *.hpp *.cpp

ident.C: $(IDENT_AWK) $(IDENT_DEPS) Makefile.am
	rm -f ident.C
	echo // do not edit. autogenerated by Makefile.am > ident.C.new 
	cat $(IDENT_DEPS) | $(AWK) -f $(IDENT_AWK) -v project=xalan_patched >> ident.C.new 
	mv ident.C.new ident.C
